<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myapplicationdev.android.quizapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Naruto Quiz"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizApp">
        <activity android:name=".HardStartActivity"></activity>
        <activity android:name=".EasyStartActivity" />
        <activity android:name=".MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EasyResultActivity" />
        <activity android:name=".EasyQuizActivity"></activity>
        <activity android:name=".HardQuizActivity"></activity>
        <activity android:name=".HardResultActivity"></activity>
        <service android:enabled="true" android:name=".BackgroundSoundService" />
    </application>

</manifest>